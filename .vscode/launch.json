{
	"configurations":
		[

			{
				"args": [ "function-arrow/argument-of-parameter/nested" ],
				"cwd": "${workspaceFolder}/Analyzers/JavaScript/Files/",
				"name": "analyze file test(s)",
				"program": "${workspaceFolder}/Analyzers/JavaScript/Files/test.js",
				"request": "launch",
				"type": "node",
				"useWSL": true
			},
			{
				"cwd": "${workspaceFolder}/Analyzers/JavaScript/Files/",
				"name": "analyze jest",
				"program": "${workspaceRoot}/node_modules/jest/bin/jest.js",
				"request": "launch",
				"type": "node",
				"useWSL": true
			},
			{
				"name": "analyze test harness",
				"type": "chrome",
				"request": "launch",
				"file": "${workspaceFolder}/Analyzers/JavaScript/Files/harness.html"
			},
			{
				"cwd": "${workspaceFolder}/Renderer/",
				"name": "renderer jest",
				"program": "${workspaceRoot}/node_modules/jest/bin/jest.js",
				"request": "launch",
				"type": "node",
				"useWSL": true
			},
			{
				"args": [ ],
				"cwd": "${workspaceFolder}/Renderer/",
				"name": "renderer test file(s)",
				"program": "${workspaceFolder}/Renderer/test.js",
				"request": "launch",
				"type": "node",
				"useWSL": true
			},
			{
				"file": "${workspaceFolder}/Renderer/harness.html",
				"name": "renderer test harness",
				"request": "launch",
				"type": "chrome"
			}
		],
	"version": "0.2.0"
}