{
	"dependencies": {
		"acorn": "5.6.2",
		"js-yaml": "3.11.0",
		"minimist": "1.2.0",
		"react": "16.4.0",
		"react-dom": "16.4.0",
		"string-pixel-width": "1.7.0"
	},
	"devDependencies": {
		"@types/acorn": "4.0.3",
		"@types/jest": "22.2.3",
		"@types/js-yaml": "3.11.1",
		"@types/react": "16.3.14",
		"@types/react-dom": "16.0.5",
		"babel-cli": "6.26.0",
		"babel-loader": "7.1.4",
		"babel-plugin-transform-object-rest-spread": "6.26.0",
		"babel-preset-env": "1.7.0",
		"coveralls": "3.0.1",
		"eslint": "4.19.1",
		"file-loader": "1.1.11",
		"jest": "23.0.1",
		"react-reflex": "2.2.8",
		"webpack": "4.11.1",
		"webpack-cli": "3.0.3"
	},
	"jest": {
		"collectCoverage": true,
		"collectCoverageFrom": [
			"**/*.js",
			"!**/node_modules/**",
			"!**/harness.js",
			"!**/*.testcases/**",
			"!**/*.testcases/**/.js",
			"!**/webpack.config.js",
			"!Harness/**",
			"!Tests/**"
		],
		"coverageDirectory": "Tests/Coverage",
		"coverageThreshold": {
			"global": {
				"branches": 100,
				"functions": 100,
				"lines": 100
			}
		}
	},
	"scripts": {
		"harness": "(cd Analyzers/JavaScript; npx webpack -p); (cd Renderer; npx webpack -p); npx webpack -p",
		"lint": "eslint .",
		"test": "jest --no-cache --runInBand",
		"test-travis": "jest --no-cache --runInBand && cat ./Tests/Coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
	}
}
